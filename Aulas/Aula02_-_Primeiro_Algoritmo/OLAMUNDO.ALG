Algoritmo "OlaMundo"

// Disciplina  : Algoritmos
// Professor   : Gustavo Guanabara
// Descrição   : Aula 2 - Primeiro Algoritmo
// Autor(a)    : Wady Jorge
// Ano         : 2024

Var

	// Variáveis
	msg: caractere
	msg2: caractere
   
Inicio

	// Funções 
	msg <- "Olá, mundo!"
	msg2 <- "Me livrei da maldição!"
	Escreval (msg)
	Escreva (msg2)
   
Fimalgoritmo